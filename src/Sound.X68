PLAYBG:
    MOVE.L  #BGMUSICREFNO, D1
    MOVE.L  #1, D2                  * Loop
    MOVE.L  #PLAY_SOUND_TASK, D0
    TRAP    #TRAPCODE   
    RTS
    
PLAYRANDOMLOSSSOUND:
    MOVE.L  SOUNDSEED, D0
    MULU    #VALUE_A, D0            ; Using the same value from Item random generation
    ADD.L   #VALUE_C, D0            ; Using the same value from Item random generation
    AND.L   #LOSSANDBITS, D0        ; Prevent divide overflow
    DIVU    #NUMLOSSSOUNDS, D0
    MOVE.L  D0, SOUNDSEED
    CLR.W   D0
    SWAP    D0
    ADD.L   #1, D0                  ; Skip the background
    
    MOVE.L  D0, D1
    MOVE.L  #PLAY_SOUND_NO_OPTIONS_TASK, D0
    TRAP    #TRAPCODE   
    
    RTS
    
PLAYRANDOMWINSOUND:
    MOVE.L  SOUNDSEED, D0
    MULU    #VALUE_A, D0        
    ADD.L   #VALUE_C, D0        
    AND.L   #WINANDBITS, D0     
    DIVU    #NUMWINSOUNDS, D0
    MOVE.L  D0, SOUNDSEED
    CLR.W   D0
    SWAP    D0
    ADD.L   #1, D0                  ; Skip the background
    ADD.L   #NUMLOSSSOUNDS, D0      ; Skip the loss sounds
    
    MOVE.L  D0, D1
    MOVE.L  #PLAY_SOUND_NO_OPTIONS_TASK, D0
    TRAP    #TRAPCODE   
    
    RTS
    
PLAYRANDOMCOLLECTSOUND:

*   Initially we were playing both the black box and yellow box collect sounds
*   on pickup. Unfortunately the dialog is not all that clear, so we play just
*   one of them randomly.

*   * Play the black box sound first
*    MOVE.L  SOUNDSEED, D0
*    MULU    #VALUE_A, D0        
*    ADD.L   #VALUE_C, D0        
*    AND.L   #BLACKCOLLECTANDBITS, D0  
*    DIVU    #NUMBLACKCOLLECTSOUNDS, D0
*    MOVE.L  D0, SOUNDSEED
*    CLR.W   D0
*    SWAP    D0
*    ADD.L   #1, D0              
*    ADD.L   #NUMLOSSSOUNDS, D0    
*    ADD.L   #NUMWINSOUNDS, D0   
*   
*    MOVE.L  D0, D1
*    MOVE.L  #PLAY_SOUND_NO_OPTIONS_TASK, D0
*    TRAP    #TRAPCODE     
*    
*   * PLAY THE YELLOW BOX SOUND NEXT
*    MOVE.L  SOUNDSEED, D0
*    MULU    #VALUE_A, D0        
*    ADD.L   #VALUE_C, D0       
*    AND.L   #YELLOWCOLLECTANDBITS, D0    
*    DIVU    #NUMYELLOWCOLLECTSOUNDS, D0
*    MOVE.L  D0, SOUNDSEED
*    CLR.W   D0
*    SWAP    D0
*    ADD.L   #1, D0              
*    ADD.L   #NUMLOSSSOUNDS, D0  
*    ADD.L   #NUMWINSOUNDS, D0   
*    ADD.L   #NUMBLACKCOLLECTSOUNDS, D0  
*   
*    MOVE.L  D0, D1
*    MOVE.L  #PLAY_SOUND_NO_OPTIONS_TASK, D0
*    TRAP    #TRAPCODE     
*    RTS
   
    MOVE.L  SOUNDSEED, D0
    MULU    #VALUE_A, D0        
    ADD.L   #VALUE_C, D0        
    AND.L   #TOTALCOLLECTANDBITS, D0     
    DIVU    #NUMTOTALCOLLECTSOUNDS, D0
    MOVE.L  D0, SOUNDSEED
    CLR.W   D0
    SWAP    D0
    ADD.L   #1, D0              
    ADD.L   #NUMLOSSSOUNDS, D0    
    ADD.L   #NUMWINSOUNDS, D0    
   
    MOVE.L  D0, D1
    MOVE.L  #PLAY_SOUND_NO_OPTIONS_TASK, D0
    TRAP    #TRAPCODE     
    RTS
    
    
    
PLAYRANDOMSTARTSOUND:
    MOVE.L  SOUNDSEED, D0
    MULU    #VALUE_A, D0       
    ADD.L   #VALUE_C, D0        
    AND.L   #STARTANDBITS, D0    
    DIVU    #NUMSTARTSOUNDS, D0
    MOVE.L  D0, SOUNDSEED
    CLR.W   D0
    SWAP    D0
    ADD.L   #1, D0              
    ADD.L   #NUMLOSSSOUNDS, D0  
    ADD.L   #NUMWINSOUNDS, D0   
    ADD.L   #NUMTOTALCOLLECTSOUNDS, D0   
    
    MOVE.L  D0, D1
    MOVE.L  #PLAY_SOUND_NO_OPTIONS_TASK, D0
    TRAP    #TRAPCODE   
    
    RTS
    
