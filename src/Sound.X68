PLAY_SOUND_NO_OPTIONS_TASK  EQU 75
   
    SECTION CODE
LOADSOUNDS:

    BSR     INITSOUNDRANDOMIZER
    
    LEA     BGMUSIC, A1
    CLR.L   D1
    MOVE.B  #BGMUSICREFNO, D1
    MOVE.L  #LOAD_SOUND_TASK, D0
    TRAP    #TRAPCODE
    
    BSR     LOADGAMELOSTSOUNDS
    BSR     LOADGAMEWONSOUNDS
    BSR     LOADBLACKCOLLECTSOUNDS
    BSR     LOADYELLOWCOLLECTSOUNDS
    BSR     LOADSTARTSOUNDS
    
    RTS
    
INITSOUNDRANDOMIZER:
    MOVE.B  #GETTIMETASK, D0
    TRAP    #TRAPCODE
    MOVE.L  D1, SOUNDSEED
    RTS
    
